@model string

<!-- Agrega un botón o enlace que, al hacer clic, abrirá una nueva pestaña o ventana con el PDF -->
<a href="javascript:void(0);" onclick="openPdfInNewTab('@Model')">Abrir PDF en nueva pestaña</a>

<script>
    function openPdfInNewTab(pdfUrl) {
        // Abre una nueva pestaña o ventana con el PDF
        window.open(pdfUrl, '_blank');
    }
</script>
